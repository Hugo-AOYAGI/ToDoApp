{
  "name": "remind",
  "version": "1.0.0",
  "description": "Just a normal To Do App.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"ReMind\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds"
  },
  "author": "Hugo AOYAGI",
  "license": "ISC",
  "dependencies": {
    "electron": "^5.0.5",
    "jquery": "^3.4.1",
    "sass": "^1.22.0",
    "weeknumber": "^1.1.1"
  },
  "devDependencies": {
    "electron-packager": "^14.0.3"
  }
}
